{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "inputs": ["../../.env"],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "dev": {
      "inputs": ["../../.env"],
      "dependsOn": ["^dev"],
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"]
    }
  }
}
